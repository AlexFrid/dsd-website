---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../config';
import BaseButton from '../components/BaseButton.astro';
import { Icon } from 'astro-icon';

---

<!DOCTYPE html>
<html lang="en">
	<head> <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} /> </head>
	<body>
		<Header />
		<main>
			<section class="container flex flex-col px-6 py-10 mx-auto space-y-6 lg:h-[32rem] lg:py-16 lg:flex-row lg:items-center">
				<div class="w-full lg:w-1/2">
					<div class="lg:max-w-lg">
						<h1 class="text-3xl font-bold tracking-wide text-gray-600 dark:text-white lg:text-5xl">
							<span class="text-blue-600 dark:text-blue-400">Hi I'm Alexander</span><br> your friendly neighbourhood<br>analytics engineer
						</h1>
						<div class="mt-8 mr-3 space-y-5">
							<p class="mt-2 text-sm text-gray-500 dark:text-gray-400 lg:text-base">I value simplicity, curiosity and automation.<br>If you share those values then <a class="underline" href="mailto:post@alexanderfridriksson.com">get in touch! <Icon class="ml-1 inline" name="ic:outline-email" width="24px" height="24px" /></a></p>
							<BaseButton> <a href="https://drive.google.com/uc?id=15VxLhBztX0rP5XGMdGCFDBJOiLOjoRIg&export=download">Download CV</a> </BaseButton>
						</div>
					</div>
				</div>
				<div class="flex items-center justify-center w-full h-full lg:w-1/2">
					<img class="object-cover w-full mx-auto rounded-md" src="../images/alexander-fridriksson.jpg" alt="Alexander Fridriksson">
				</div>
			</section>
			<section class="max-w-screen-xl mx-auto mt-10 mb-10">
				<p class=" mt-4 mb-4 font-bold text-gray-600 xl:mt-6 dark:text-gray-300 text-center">
